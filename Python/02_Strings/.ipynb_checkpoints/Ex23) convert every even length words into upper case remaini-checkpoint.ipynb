    "    l=[]\n",
    "    for i in s.split(): \n",
    "        if (len(i))%2==0:\n",
    "            l.append(i.upper())\n",
    "        # else:\n",
    "            # l.append(i[0].lower())\n",
    "        \n",
    "    return \" \".join(l)\n",
    "s=\"rohit  santosh a ranvir vandana hi\"\n",
    "fun(s)"
