    "def fun(s):\n",
    "    l=[]\n",
    "    for i in s.split():        \n",
    "        l.append(i[0].upper()+i[1:len(i)-1].lower()+i[-1].upper())\n",
    "    return \" \".join(l)\n",
    "s=\"rohit A santosh ranvir vandana\"\n",
    "fun(s)"
