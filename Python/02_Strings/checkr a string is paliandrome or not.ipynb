   "source": [
    "def fun(s):\n",
    "    low=0\n",
    "    high=len(s)-1\n",
    "    while low<=high:\n",
    "        if s[low]!=s[high]:\n",
    "            return False\n",
    "        low+=1\n",
    "        high-=1\n",
    "    return True\n",
    "s=\"nayan\"\n",
    "fun(s)"
   ]
