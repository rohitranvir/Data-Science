   "source": [
    "def fun(s):\n",
    "    res=\"\"\n",
    "    for i in s:\n",
    "        if i.isupper():\n",
    "            res=res+chr(ord(i)+32)\n",
    "        if i.islower():\n",
    "            res=res+chr(ord(i)-32)\n",
    "    return res\n",
    "s=\"RoHit\"\n",
    "fun(s)"
   ]
