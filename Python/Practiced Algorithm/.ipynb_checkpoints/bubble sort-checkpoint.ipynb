    "def bubblesort(l):\n",
    "    for i in range(len(l)-1):\n",
    "        for j in range(len(l)-i-1):\n",
    "            if l[j]<l[j+1]:\n",
    "                l[j],l[j+1]=l[j+1],l[j]\n",
    "    return l\n",
    "            \n",
    "l=[10,51,86,2,15] \n",
    "bubblesort(l)"
   ]
  }
