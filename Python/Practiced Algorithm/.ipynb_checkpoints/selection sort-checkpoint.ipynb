    "def selectionsort(l):\n",
    "    for i in range(len(l)-1):\n",
    "        min_i=i\n",
    "        for j in range((i+1),len(l)):\n",
    "            if l[min_i]>l[j]:\n",
    "                min_i=j\n",
    "        l[i],l[min_i]=l[min_i],l[i]\n",
    "    return l\n",
    "l=[10,51,86,2,15] \n",
    "selectionsort(l)"
   ]
  },
