   "source": [
    "def merg(a,b):\n",
    "    c=[]\n",
    "    i,j=0,0\n",
    "    while i<len(a) and j<len(b):\n",
    "        if a[i]<b[j]:\n",
    "            c.append(a[i])\n",
    "            i+=1\n",
    "        else:\n",
    "            c.append(b[j])\n",
    "            j+=1\n",
    "    c.extend(a[i:])\n",
    "    c.extend(b[j:])\n",
    "    print(c)\n",
    "\n",
    "a=[1,3,5,7,9]\n",
    "b=[2,4,6,8,10]\n",
    "merg(a,b)"
   ]
  }
