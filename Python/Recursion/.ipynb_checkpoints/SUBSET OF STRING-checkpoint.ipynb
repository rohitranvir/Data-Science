    "def subset(s,ans,index):\n",
    "    if index==len(s):\n",
    "        if len(ans)==0:\n",
    "            print(\"NUll\")\n",
    "        else:\n",
    "            print(ans)\n",
    "        return\n",
    "    subset(s,ans+s[index],index+1)\n",
    "    subset(s,ans,index+1)\n",
    "subset(\"abc\",\"\",0)"
