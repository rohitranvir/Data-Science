   "source": [
    "def gridways(i,j,m,n):\n",
    "    if i==m-1 and j==n-1:\n",
    "        return 1\n",
    "    if i==m or j==n:\n",
    "        return 0\n",
    "    val1=gridways(i+1,j,m,n)\n",
    "    val2=gridways(i,j+1,m,n)\n",
    "    return val1+val2\n",
    "m=3\n",
    "n=3\n",
    "print(gridways(0,0,m,n))"
   ]
