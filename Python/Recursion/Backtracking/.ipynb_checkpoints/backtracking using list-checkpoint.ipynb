    "def change(l,index,value):\n",
    "    if len(l)==index:\n",
    "        return \n",
    "    l[index]=value\n",
    "    change(l,index+1,value+1)\n",
    "    l[index]=l[index]-2\n",
    "l=[0,0,0,0,0]\n",
    "print(l)\n",
    "change(l,0,1)\n",
    "print(l)"
