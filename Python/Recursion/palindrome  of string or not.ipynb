    "def pali(s,start,end):\n",
    "    if start>=end:\n",
    "        return True\n",
    "    return s[start]==s[end-1] and pali(s,start+1,end-1) \n",
    "s=\"madam\"\n",
    "start=0\n",
    "end=len(s)\n",
    "pali(s,start,end)"
