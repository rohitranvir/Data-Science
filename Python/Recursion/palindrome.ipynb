   "source": [
    "def pali(s,start,end):\n",
    "    if start>=end:\n",
    "        return True\n",
    "    return s[start]==s[end] and pali(s,start+1,end-1)\n",
    "s=\"madam\"\n",
    "s=\"sir\"\n",
    "pali(s,0,len(s)-1)"
   ]
