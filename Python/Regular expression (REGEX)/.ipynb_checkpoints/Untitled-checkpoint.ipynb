    "import re\n",
    "\n",
    "text = \"My age is 21 and my brother is 18\"\n",
    "\n",
    "matches = re.finditer(r\"\\d+\", text)\n",
    "for m in matches:\n",
    "    print(m.group(), m.start(), m.end())"
