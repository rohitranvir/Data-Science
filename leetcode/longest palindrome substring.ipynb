    "def longestPalindrome( s):\n",
    "        start=0\n",
    "        end=len(s)-1\n",
    "        while start <=end:\n",
    "            if s[start] !=s[end]:                \n",
    "                continue\n",
    "            \n",
    "            start+=1\n",
    "            end-=1\n",
    "        return s[1:-1]\n",
    "s=\"cbbd\"\n",
    "longestPalindrome(s)"
   ]
