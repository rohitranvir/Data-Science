    "def reverse(x):\n",
    "    sign =-1 if x<0 else 1\n",
    "    n=abs(x)\n",
    "    re=0\n",
    "    while n>0:\n",
    "        t=n%10\n",
    "        n=n//10\n",
    "        re=re*10+t\n",
    "        \n",
    "        \n",
    "    return sign*re\n",
    "x=901000\n",
    "reverse(x)"
   ]
  }
